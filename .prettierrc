{
  "jsxSingleQuote": true,
  "singleQuote": true,
  "useTabs": false,
  "tabWidth": 2,
  "trailingComma": "all",
  "arrowParens": "always",
  "semi": true,
  "bracketSpacing": true,
  "endOfLine": "auto",
  "printWidth": 80,
  "proseWrap": "always",
  "plugins": [
    "prettier-plugin-tailwindcss"
  ]
}